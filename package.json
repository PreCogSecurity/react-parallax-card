{
  "name": "react-parallax-card",
  "version": "0.1.0",
  "author": "Theo Pak <theopak@gmail.com (https://github.com/theopak)",
  "homepage": "https://theopak.github.io/react-parallax-card",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/theopak/react-parallax-card.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "cover": "npm run test -- --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint **/*.js --ignore-pattern '/dist/*'"
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "globals": {
      "__DEV__": true
    },
    "options": {
      "failOnError": true
    },
    "extends": [
      "standard",
      "standard-react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "object-curly-spacing": [
        "warn",
        "always"
      ]
    }
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "react-scripts": "0.8.5"
  }
}
